#include <linux/types.h>
#include <linux/kernel.h>

int main() {
    printk(KERN_INFO "sizeof(s8): %lu\n", sizeof(s8));
    printk(KERN_INFO "sizeof(u8): %lu\n", sizeof(u8));
    printk(KERN_INFO "sizeof(s16): %lu\n", sizeof(s16));
    printk(KERN_INFO "sizeof(u16): %lu\n", sizeof(u16));
    printk(KERN_INFO "sizeof(s32): %lu\n", sizeof(s32));
    printk(KERN_INFO "sizeof(u32): %lu\n", sizeof(u32));
    printk(KERN_INFO "sizeof(s64): %lu\n", sizeof(s64));
    printk(KERN_INFO "sizeof(u64): %lu\n", sizeof(u64));

    return 0;
}
